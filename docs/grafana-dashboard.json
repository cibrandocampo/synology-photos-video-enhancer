      "targets": [
        {
          "queryText": "WITH r AS (\n  SELECT\n    transcoded_video_resolution AS resolution,\n    CAST(substr(transcoded_video_resolution, 1, instr(transcoded_video_resolution, 'x') - 1) AS INTEGER) AS w,\n    CAST(substr(transcoded_video_resolution, instr(transcoded_video_resolution, 'x') + 1) AS INTEGER) AS h\n  FROM transcodings\n  WHERE transcoded_video_resolution IS NOT NULL\n    AND TRIM(transcoded_video_resolution) != ''\n    AND transcoded_video_resolution != '0x0'\n)\nSELECT\n  resolution,\n  CASE\n    WHEN w > h THEN 'horizontal'\n    WHEN w < h THEN 'vertical'\n    ELSE 'square'\n  END AS orientation,\n  ROUND(1.0 * w / h, 2) AS aspect_ratio,\n  COUNT(*) AS total\nFROM r\nGROUP BY resolution, orientation, aspect_ratio\nORDER BY total DESC;\n",
          "queryType": "table",
          "rawQueryText": "WITH r AS (\n  SELECT\n    transcoded_video_resolution AS resolution,\n    CAST(substr(transcoded_video_resolution, 1, instr(transcoded_video_resolution, 'x') - 1) AS INTEGER) AS w,\n    CAST(substr(transcoded_video_resolution, instr(transcoded_video_resolution, 'x') + 1) AS INTEGER) AS h\n  FROM transcodings\n  WHERE transcoded_video_resolution IS NOT NULL\n    AND TRIM(transcoded_video_resolution) != ''\n    AND transcoded_video_resolution != '0x0'\n)\nSELECT\n  resolution,\n  CASE\n    WHEN w > h THEN 'horizontal'\n    WHEN w < h THEN 'vertical'\n    ELSE 'square'\n  END AS orientation,\n  ROUND(1.0 * w / h, 2) AS aspect_ratio,\n  COUNT(*) AS total\nFROM r\nGROUP BY resolution, orientation, aspect_ratio\nORDER BY total DESC;\n",
          "refId": "A",
          "timeColumns": [
            "time",
            "ts"
          ]
        }
      ],
      "title": "Transcoded video resolutions",
      "type": "table"
